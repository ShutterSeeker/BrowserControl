    <script type="text/javascript">
        var firstTouch = true;
        $(document).ready(function () {
            DisplayResourceKey();
            $.fn.modal.Constructor.prototype.enforceFocus = function () { };
            //Prevent touch device keypad popup issue on page load
            $(document).on('touchstart', function () {
                //Reset focus on first touch
                if (firstTouch && document.activeElement) {
                    firstTouch = false;
                    document.activeElement.blur();
                    document.activeElement.focus();
                }

            });
        });

        $(function () {
            if (typeof _webUi.dialog.showModalDialogByEvent === 'function') {
                $(document).delegate('#MenuRestoreDefaultSettings', 'click', { 'ModalDialogName': 'ResetSettingsModalDialog' }, _webUi.dialog.showModalDialogByEvent)
                $(document).on('click', '#ConfigureWorkStation', function () {

                    _webUi.WebUi.processWorkStationAssignment();
                });
            }
            var actionsMenu = $('a#InsightMenuActionsDropdown[data-toggle="dropdown"]').parent();
            var printMenu = $('a#InsightMenuPrintActionsDropdown[data-toggle="dropdown"]').parent();
            var configMenu = $('a#InsightMenuConfigActionsDropdown[data-toggle="dropdown"]').parent();
            if (actionsMenu)
                actionsMenu.on("show.bs.dropdown", _webUi.navigationHeader.onActionsMenuPopup);
            if (printMenu)
                printMenu.on("show.bs.dropdown", _webUi.navigationHeader.onActionsMenuPopup);
            if (configMenu)
                configMenu.on("show.bs.dropdown", _webUi.navigationHeader.onActionsMenuPopup);
        });

    </script>
