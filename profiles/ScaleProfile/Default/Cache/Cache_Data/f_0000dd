                                        <li class="dropdown pull-left pullleftfavourite" ><a class="dropdown-toggle" data-toggle="dropdown" id="InsightMenuFavoritesDropdown" href=""><span class="fas fa-star starIcon"></span></a><ul class="dropdown-menu" data-bind="foreach:savedSearches"><li class="dropdownaction" data-controlType="SavedSearchesMenu"><a id="SearchPaneMenuFavoritesChooseSearch" href="javascript:;"><span class="deletesavedsearchtext col-xs-6 col-md-6 col-lg-2 vcenter" data-bind="text:$data"></span><span class="deletesavedsearchbutton navbar-right glyphicon glyphicon-remove-circle"></span> </a></li></ul>

<script ">
$(function () {$(document).on('click', "[data-controlType='SavedSearchesMenu']", function(e) { _webUi.insightSearchPaneActions.applySavedSearchClicked(e);})})

var savedSearches = ko.observableArray(["Palletizing"]);
ko.applyBindings(savedSearches);

function setSavedSearchesList(newSearchName) {
	if (newSearchName.length > 0 && (savedSearches.peek().length === 0 || savedSearches.peek().filter(function(element){return element.toLowerCase() === newSearchName.toLowerCase()}).length <= 0)) {
		savedSearches.push(newSearchName);
		savedSearches.sort(function(a, b) { if (a.toLowerCase() < b.toLowerCase()) return -1; if (a.toLowerCase() > b.toLowerCase()) return 1; return 0; });
	}
}

function deleteSavedSearch(searchName) {
	if (searchName.length > 0 && savedSearches.peek().length > 0 && savedSearches.peek().toString().toLowerCase().indexOf(searchName.toLowerCase()) >= 0) {
		savedSearches.remove(searchName);
	}
}

</script>


                                        

